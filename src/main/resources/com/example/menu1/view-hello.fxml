<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="myPane" alignment="CENTER" prefHeight="366.0" prefWidth="295.0" spacing="20.0" style="-fx-border-radius: 20;" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MeinController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Label text="Witaj w naszej aplikacji" />
    <Button fx:id="login" mnemonicParsing="true" onAction="#onClickButtonLogin" text="Zaloguj się" />
   <Button fx:id="register" mnemonicParsing="true" onAction="#onClickButtonRegister" text="zarejestruj się" />
   <Button fx:id="exit" mnemonicParsing="false" onAction="#onClickButtonExit" text="Wyjscie" />
</VBox>
